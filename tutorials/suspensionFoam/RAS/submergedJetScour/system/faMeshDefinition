{
    apronFa
    {
        type patch;
        neighbourPolyPatch rigidWalls;
    }

    outletFa
    {
        type patch;
        neighbourPolyPatch outlet;
    }

    sidesFa
    {
        type empty;
        neighbourPolyPatch sidesBed;
    }
}

