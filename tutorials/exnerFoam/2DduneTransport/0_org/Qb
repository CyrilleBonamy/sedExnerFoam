FoamFile
{
    version     2.0;
    format      ascii;
    class       areaVectorField;
    object      Qb;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [0 2 -1 0 0 0 0];

internalField       uniform (0.05 0 0);

boundaryField
{

    inletFa
    {
        type        fixedGradient;
        gradient    uniform (0 0 0);
    }

    outletFa
    {
        type        fixedGradient;
        gradient    uniform (0 0 0);
    }

    sidesFa
    {
        type        symmetry;
    }
}


