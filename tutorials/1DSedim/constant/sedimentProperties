FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      sedimentProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

rhoS    [1 -3 0 0 0 0 0] 1600; // sediment density

dS      [0 1 0 0 0 0 0] 0.001; // sediment diameter

fallModel
{
	type   Fixed;
        value  0.003;
}

hindranceModel
{
	type   Zaki;
	n      5;
}

