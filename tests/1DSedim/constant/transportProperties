FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}

// * * * * * * * * * * * * sediment properties * * * * * * * * * * * * //

rhoS    [1 -3 0 0 0 0 0] 1050; // sediment density

dS      [0 1 0 0 0 0 0] 2.9e-4; // sediment diameter

Cmax    [0 0 0 0 0 0 0] 0.6; // max volumic sediment concentration

// ************************************************************************* //

transportModel  Newtonian;

nu      nu [ 0 2 -1 0 0 0 0 ] 2.105e-05;

Sc      1; // Schmidt number

