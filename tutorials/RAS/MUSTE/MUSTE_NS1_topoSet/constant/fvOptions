FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

momentumSource
{
    type            meanVelocityForce;

    selectionMode   all;

    fields          (U);
    Ubar            (0.813 0 0);
}

fixedC
{
    type            scalarFixedValueConstraint;
    active          yes;

    selectionMode   cellZone;
    cellZone        referenceLevel;
    fieldValues
    {
        Cs           0.011;
    }
}



