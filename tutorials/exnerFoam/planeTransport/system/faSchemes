FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      faSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default            Euler;
}

gradSchemes
{
    default            Gauss linear;
}

divSchemes
{
    default            none;
    div(phis,Zbf)       Gauss upwind phis;
}

laplacianSchemes
{
    default            none;
}

interpolationSchemes
{
    default            linear;
}

lnGradSchemes
{
    default            corrected;
}


