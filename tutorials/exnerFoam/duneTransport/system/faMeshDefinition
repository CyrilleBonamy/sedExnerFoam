FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      faMeshDefinition;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

polyMeshPatches ( "bottom" );

boundary
{
    inlet
    {
        type cyclic;
	ownerPolyPatch bottom;
        neighbourPolyPatch outlet;
    }

    outlet
    {
        type cyclic;
	ownerPolyPatch bottom;
        neighbourPolyPatch inlet;
    }

    sides
    {
        type empty;
	ownerPolyPatch bottom;
        neighbourPolyPatch sides;
    }
}

defaultPatch
{
    name empty;
    type empty;
}


