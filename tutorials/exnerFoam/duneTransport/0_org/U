FoamFile
{
    version     2.0;
    format      ascii;
    class       areaVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [0 1 -1 0 0 0 0];

internalField       uniform (0.05 0 0);

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type        cyclic;
    }

    outlet
    {
        type        cyclic;
    }

    sides
    {
        type        empty;
    }
}


